{
  "name": "@vscode/jupyter-lsp-middleware",
  "version": "0.0.1",
  "description": "VS Code Python Language Server Middleware for Jupyter Notebook",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "compile": "tsc -p ./",
    "copyTestAssets": "node ./scripts/prepareTest.js",
    "compilewatch": "tsc -watch -p ./",
    "buildTests": "npm run download-api && npm run copyTestAssets && npm run compilewatch",
    "test": "npm run compile && npm run copyTestAssets && node ./out/test/runTest.js",
    "download-api": "vscode-dts dev",
    "postdownload-api": "vscode-dts main",
    "webpack": "webpack --mode production && node ./scripts/optimizeTypings.js",
    "webpack-link": "webpack --mode development && node ./scripts/optimizeTypings.js",
    "webpack-dev": "webpack --mode development --watch",
    "prepare": "npm run download-api",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "engines": {
    "vscode": "1.67.0-insider"
  },
  "enabledApiProposals": [
    "notebookEditorEdit",
    "notebookEditor"
  ],
  "dependencies": {
    "@vscode/lsp-notebook-concat": "0.1.17",
    "fast-myers-diff": "3.1.0",
    "sha.js": "2.4.11",
    "vscode-languageclient": "9.0.1",
    "vscode-languageserver-protocol": "3.17.5",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/chai": "4.3.9",
    "@types/fs-extra": "11.0.3",
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.3",
    "@types/node": "20.8.9",
    "@types/sha.js": "2.4.3",
    "@types/sinon": "10.0.20",
    "@types/tmp": "0.2.5",
    "@types/uuid": "9.0.6",
    "chai": "4.3.10",
    "fs-extra": "11.1.1",
    "glob": "10.3.10",
    "mocha": "10.2.0",
    "screenshot-desktop": "1.15.0",
    "sinon": "17.0.0",
    "tmp": "0.2.1",
    "ts-loader": "9.5.0",
    "typescript": "5.2.2",
    "uuid": "9.0.1",
    "vscode-dts": "0.3.3",
    "vscode-test": "1.3.0",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "typescript-esbuild": "0.3.1"
  },
  "overrides": {
    "terser@<5.14.2": "5.14.2"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  }
}
