{
	"name": "@vscode/jupyter-lsp-middleware",
	"version": "0.0.1",
	"private": false,
	"description": "VS Code Python Language Server Middleware for Jupyter Notebook",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandJupyterLSPMiddleware#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandJupyterLSPMiddleware/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandJupyterLSPMiddleware.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "dist/node/index.js",
	"types": "dist/node/index.d.ts",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"buildTests": "npm run download-api && npm run copyTestAssets && npm run compilewatch",
		"compile": "tsc -p ./",
		"compilewatch": "tsc -watch -p ./",
		"copyTestAssets": "node ./scripts/prepareTest.js",
		"download-api": "vscode-dts dev",
		"postdownload-api": "vscode-dts main",
		"prepare": "npm run download-api",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"webpack": "webpack --mode production && node ./scripts/optimizeTypings.js",
		"webpack-dev": "webpack --mode development --watch",
		"webpack-link": "webpack --mode development && node ./scripts/optimizeTypings.js"
	},
	"dependencies": {
		"fast-myers-diff": "3.2.0",
		"sha.js": "2.4.11"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/chai": "4.3.16",
		"@types/fs-extra": "11.0.4",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.10",
		"@types/sha.js": "2.4.4",
		"@types/sinon": "17.0.3",
		"@types/tmp": "0.2.6",
		"@types/uuid": "9.0.8",
		"chai": "5.1.0",
		"fs-extra": "11.2.0",
		"glob": "10.3.12",
		"screenshot-desktop": "1.15.0",
		"sinon": "17.0.1",
		"tmp": "0.2.3",
		"ts-loader": "9.5.1",
		"uuid": "9.0.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": [
		"notebookEditorEdit",
		"notebookEditor"
	],
	"overrides": {
		"terser@<5.14.2": "5.14.2"
	}
}
